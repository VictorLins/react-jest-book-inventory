{
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx"
    ]
  }